<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="wrap_content"
  android:layout_height="wrap_content"
  android:orientation="vertical">
<TextView android:text="Title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/receipt_detail_title" android:textSize="16sp"></TextView>
<TextView android:text="date" android:id="@+id/receipt_detail_date" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
<TextView android:text="description" android:id="@+id/receipt_detail_description" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>

</LinearLayout>
